        "responses": {
          "200": {
            "description": "(streaming responses)",
            "schema": {
              "$ref": "#/definitions/gitalyCommitDiffResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/gitalyCommitDiffRequest"
            }
          }
        ],
        "tags": [
          "Diff"
        ]
